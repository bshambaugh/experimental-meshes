FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1383";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 31904.8;

boundaryField
{
    inletFace
    {
        type            zeroGradient;
    }
    inletWall
    {
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        type            epsilonWallFunction;
        value           uniform 31904.8;
    }
    inletsourceFace
    {
        type            zeroGradient;
    }
    inletdestinationFace
    {
        type            zeroGradient;
    }
    fineMeshWall
    {
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        type            epsilonWallFunction;
        value           uniform 31904.8;
    }
    fineMeshSourceFace
    {
        type            zeroGradient;
    }
    fineMeshdestinationFace
    {
        type            zeroGradient;
    }
    fineMeshSymmetryWall
    {
        type            symmetryPlane;
    }
    fineMeshOutletLeft
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 1e-15;
    }
    fineMeshOutletBottom
    {
        type            zeroGradient;
    }
}


